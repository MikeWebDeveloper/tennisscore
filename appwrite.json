{
    "projectId": "68460965002524f1942e",
    "projectName": "tennislivescore",
    "settings": {
        "services": {
            "account": true,
            "avatars": true,
            "databases": true,
            "locale": true,
            "health": true,
            "storage": true,
            "teams": true,
            "users": true,
            "functions": true,
            "graphql": true,
            "messaging": true
        },
        "auth": {
            "methods": {
                "jwt": true,
                "phone": true,
                "invites": true,
                "anonymous": true,
                "email-otp": true,
                "magic-url": true,
                "email-password": true
            },
            "security": {
                "duration": 31536000,
                "limit": 0,
                "sessionsLimit": 10,
                "passwordHistory": 0,
                "passwordDictionary": false,
                "personalDataCheck": false,
                "sessionAlerts": false,
                "mockNumbers": []
            }
        }
    },
    "databases": [
        {
            "$id": "tennisscore_db",
            "name": "TennisScore Database",
            "enabled": true
        }
    ],
    "collections": [
        {
            "$id": "players",
            "$permissions": [
                "read(\"any\")",
                "create(\"users\")",
                "update(\"users\")",
                "delete(\"users\")"
            ],
            "databaseId": "tennisscore_db",
            "name": "Players",
            "enabled": true,
            "documentSecurity": true,
            "attributes": [
                {
                    "key": "firstName",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 100,
                    "default": null
                },
                {
                    "key": "lastName",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 100,
                    "default": null
                },
                {
                    "key": "yearOfBirth",
                    "type": "integer",
                    "required": false,
                    "array": false,
                    "min": 1900,
                    "max": 2025,
                    "default": null
                },
                {
                    "key": "rating",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 50,
                    "default": null
                },
                {
                    "key": "profilePictureId",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 36,
                    "default": null
                },
                {
                    "key": "userId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 36,
                    "default": null
                },
                {
                    "key": "isMainPlayer",
                    "type": "boolean",
                    "required": false,
                    "array": false,
                    "default": false
                },
                {
                    "key": "isAnonymous",
                    "type": "boolean",
                    "required": false,
                    "array": false,
                    "default": false
                },
                {
                    "key": "club",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 100,
                    "default": ""
                },
                {
                    "key": "playingHand",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "elements": [
                        "right",
                        "left"
                    ],
                    "format": "enum",
                    "default": null
                },
                {
                    "key": "bhRating",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 20,
                    "default": null
                },
                {
                    "key": "czRanking",
                    "type": "integer",
                    "required": false,
                    "array": false,
                    "min": 1,
                    "max": 650,
                    "default": null
                }
            ],
            "indexes": [
                {
                    "key": "userId_index",
                    "type": "key",
                    "status": "available",
                    "attributes": [
                        "userId"
                    ],
                    "orders": []
                }
            ]
        },
        {
            "$id": "matches",
            "$permissions": [
                "read(\"any\")",
                "create(\"users\")",
                "update(\"users\")",
                "delete(\"users\")"
            ],
            "databaseId": "tennisscore_db",
            "name": "Matches",
            "enabled": true,
            "documentSecurity": false,
            "attributes": [
                {
                    "key": "playerOneId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 36,
                    "default": null
                },
                {
                    "key": "playerTwoId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 36,
                    "default": null
                },
                {
                    "key": "matchDate",
                    "type": "datetime",
                    "required": true,
                    "array": false,
                    "format": "",
                    "default": null
                },
                {
                    "key": "matchFormat",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 500,
                    "default": null
                },
                {
                    "key": "status",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "elements": [
                        "In Progress",
                        "Completed"
                    ],
                    "format": "enum",
                    "default": null
                },
                {
                    "key": "winnerId",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 36,
                    "default": null
                },
                {
                    "key": "score",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 2000,
                    "default": null
                },
                {
                    "key": "userId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 36,
                    "default": null
                },
                {
                    "key": "pointLog",
                    "type": "string",
                    "required": false,
                    "array": true,
                    "size": 50000,
                    "default": null
                },
                {
                    "key": "events",
                    "type": "string",
                    "required": false,
                    "array": true,
                    "size": 10000,
                    "default": null
                },
                {
                    "key": "playerThreeId",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 50,
                    "default": null
                },
                {
                    "key": "playerFourId",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 50,
                    "default": null
                },
                {
                    "key": "isDoubles",
                    "type": "boolean",
                    "required": false,
                    "array": false,
                    "default": false
                },
                {
                    "key": "detailLevel",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 20,
                    "default": "simple"
                },
                {
                    "key": "endTime",
                    "type": "datetime",
                    "required": false,
                    "array": false,
                    "format": "",
                    "default": null
                },
                {
                    "key": "retirementReason",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "startTime",
                    "type": "datetime",
                    "required": false,
                    "array": false,
                    "format": "",
                    "default": null
                },
                {
                    "key": "setDurations",
                    "type": "string",
                    "required": false,
                    "array": true,
                    "size": 2000,
                    "default": null
                },
                {
                    "key": "tournamentName",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 255,
                    "default": null
                }
            ],
            "indexes": [
                {
                    "key": "userId_index",
                    "type": "key",
                    "status": "available",
                    "attributes": [
                        "userId"
                    ],
                    "orders": []
                },
                {
                    "key": "status_date_index",
                    "type": "key",
                    "status": "available",
                    "attributes": [
                        "status",
                        "matchDate"
                    ],
                    "orders": [
                        "ASC",
                        "DESC"
                    ]
                }
            ]
        }
    ],
    "buckets": [
        {
            "$id": "profile-pictures-bucket-id",
            "$permissions": [
                "read(\"any\")",
                "create(\"users\")"
            ],
            "fileSecurity": true,
            "name": "Profile Pictures",
            "enabled": true,
            "maximumFileSize": 5242880,
            "allowedFileExtensions": [
                "jpg",
                "jpeg",
                "png",
                "webp"
            ],
            "compression": "gzip",
            "encryption": false,
            "antivirus": true
        }
    ]
}