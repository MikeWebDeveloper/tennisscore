{
  "name": "tennisscore",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true npm run build",
    "test": "echo \"No unit tests configured yet\" && exit 0",
    "test:e2e": "playwright test",
    "test:e2e:ci": "playwright test --config=playwright.config.ci.ts",
    "test:e2e:production": "BASE_URL=https://your-production-domain.vercel.app playwright test",
    "test:czech": "npm run validate:translations && playwright test --grep=\"Czech\"",
    "validate:translations": "node scripts/validate-translations.js",
    "validate:translations:verbose": "node scripts/validate-translations.js --verbose",
    "setup:appwrite": "node scripts/setup-appwrite.js",
    "investigate:quick": "node scripts/investigate-matches.js --quick",
    "investigate:detailed": "node scripts/investigate-matches.js --detailed",
    "fix": "node scripts/fix-corrupted-matches.js",
    "fix:all": "node scripts/fix-corrupted-matches.js --all",
    "perf:all": "npm run perf:analyze && npm run perf:audit && npm run perf:lighthouse",
    "perf:analyze": "node scripts/performance-analysis.js",
    "perf:audit": "node scripts/performance-audit.js",
    "perf:lighthouse": "lighthouse http://localhost:3000 --output json --output html --output-path ./performance-results/lighthouse",
    "perf:baseline": "node scripts/performance-baseline.js",
    "perf:bundle": "node scripts/bundle-analysis.js",
    "perf:pwa": "node scripts/pwa-comparison.js",
    "lucide:analyze": "node scripts/analyze-lucide-imports.js",
    "lucide:optimize": "node scripts/optimize-lucide-imports.js",
    "deploy:staging": "./scripts/deploy.sh -e staging",
    "deploy:production": "./scripts/deploy.sh -e production",
    "deploy:dry-run": "./scripts/deploy.sh -d",
    "rollback": "./scripts/rollback.sh",
    "verify:deployment": "./scripts/verify-deployment.sh",
    "monitor:uptime": "node monitoring/uptime-monitor.js",
    "security:audit": "npm audit --audit-level high && node scripts/security-check.js",
    "security:scan": "npm run security:audit && zap-baseline.py -t http://localhost:3000",
    "ci:install": "npm ci --prefer-offline --no-audit --no-fund",
    "ci:build": "npm run type-check && npm run lint && npm run build",
    "ci:test": "npm run test && npm run test:e2e",
    "ci:security": "npm run security:audit",
    "ci:deploy": "npm run ci:install && npm run ci:build && npm run ci:test && npm run ci:security",
    "postinstall": "husky install || true"
  },
  "dependencies": {
    "@appwrite.io/console": "^1.5.7",
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.25.0",
    "@tanstack/react-query-devtools": "^5.85.5",
    "appwrite": "^14.0.1",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^0.2.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.0.8",
    "jose": "^6.1.0",
    "jspdf": "^3.0.2",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.363.0",
    "next": "^14.2.5",
    "next-intl": "^3.9.1",
    "next-themes": "^0.4.6",
    "node-appwrite": "^18.0.0",
    "react": "^18.3.1",
    "react-day-picker": "^9.9.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.0",
    "react-image-crop": "^11.0.10",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "uplot": "^1.6.30",
    "uplot-react": "^1.2.4",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.0",
    "@types/node": "^20.11.24",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "husky": "^9.0.11",
    "postcss": "^8.4.35",
    "sharp": "^0.33.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/tennisscore.git"
  },
  "keywords": [
    "tennis",
    "scoring",
    "nextjs",
    "react",
    "typescript",
    "appwrite",
    "sports"
  ],
  "author": "TennisScore Development Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-username/tennisscore/issues"
  },
  "homepage": "https://your-production-domain.vercel.app"
}
